{
    "name": "MCP SRG Mapper",
    "version": "0.0.1",
    "manifest_version": 2,
    "description": "Applies MCP mappings to SRG named code snippets on github repos and pastebins.",
    "homepage_url": "https://github.com/bspkrs/MinecraftForge-Patch-Mapper",
    "icons": {
        "16": "icons/icon16.png",
        "24": "icons/icon24.png",
        "48": "icons/icon48.png",
        "64": "icons/icon64.png",
        "128": "icons/icon128.png"
    },
    "default_locale": "en",
    "permissions": [
        "storage",
        "https://export.mcpbot.bspk.rs/*"
    ],
    "content_scripts": [
        {
            "matches": [
                "https://github.com/*/MinecraftForge/*",
                "https://github.com/*/FML/*"
            ],
            "js": [
                "src/inject/inject.js",
                "js/jszip/jszip.js"
            ]
        }
    ]
}