{
    "name": "MCP SRG Mapper",
    "version": "0.1.0",
    "manifest_version": 2,
    "description": "Applies MCP mappings to SRG named code snippets on github repos and pastebins.",
    "homepage_url": "https://github.com/bspkrs/MinecraftForge-Patch-Mapper",
    "icons": {
        "16": "icons/icon16.png",
        "24": "icons/icon24.png",
        "48": "icons/icon48.png",
        "64": "icons/icon64.png",
        "128": "icons/icon128.png"
    },
    "default_locale": "en",
    "permissions": [
        "storage",
        "http://files.minecraftforge.net/maven/de/oceanlabs/mcp/*"
    ],
    "content_scripts": [
        {
            "matches": [
                "https://github.com/*/MinecraftForge/*",
                "https://github.com/*/FML/*",
                "https://pastebin.com/*",
                "http://pastebin.com/*"
            ],
            "js": [
                "src/inject.js"
            ]
        }
    ],
    "background": {
        "scripts": ["src/background.js",
            "js/jszip.js"],
        "persistent": false
    }
}